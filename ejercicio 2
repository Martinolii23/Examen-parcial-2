//multiplo 3 de 1 a 100
#include <stdio.h>
#include <math.h>

int main() {
	int numero=1;
	
    for (numero ; numero <= 100; numero++) {
        if (numero % 3 == 0) {
            printf("%d --> Es multiplo de 3 \n\n", numero);
        } else {
            printf("%d --> No es multiplo de 3\n\n", numero);
        }
    }

    return 0;
}
